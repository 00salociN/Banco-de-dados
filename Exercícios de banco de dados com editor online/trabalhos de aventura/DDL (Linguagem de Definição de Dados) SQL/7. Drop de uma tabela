Escreva uma consulta SQL para excluir permanentemente uma tabela e todos os seus dados.

-- tabela existente
create table funcionario(
    id int auto_increment,
    nomecliente varchar(100),
    idade int not null check (idade > -1),
    altura decimal(5,2) not null,
    CPF int,
    constraint PKFuncionarios primary key(id)
);

-- excluindo a tabela
drop table funcionario;